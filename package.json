{
  "name": "TODO",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "app:build": "wireit",
    "app:dev": "wireit",
    "app:start": "wireit",
    "build": "wireit",
    "ci:test": "wireit",
    "dev": "wireit",
    "lint": "wireit",
    "mailer:dev": "wireit",
    "pretty": "wireit",
    "start": "wireit",
    "test": "wireit",
    "typecheck": "wireit"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.1.0",
    "@premieroctet/next-admin": "^4.4.2",
    "@prisma/client": "^5.15.0",
    "@react-email/components": "^0.0.19",
    "@sentry/nextjs": "^8.9.2",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@zenstackhq/runtime": "^2.2.3",
    "js-md5": "^0.8.3",
    "lucide-react": "^0.395.0",
    "next": "^14.2.4",
    "next-auth": "^5.0.0-beta.18",
    "nodemailer": "^6.9.13",
    "prisma-json-schema-generator": "^5.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.3",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tslog": "^4.9.3",
    "zod": "^3.23.8",
    "zsa": "^0.3.5",
    "zsa-react": "^0.1.6"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^8.8.4",
    "@tailwindcss/typography": "^0.5.13",
    "@types/js-md5": "^0.7.2",
    "@types/node": "^20.12.12",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vercel/style-guide": "^6.0.0",
    "autoprefixer": "^10.4.19",
    "cpx2": "^7.0.1",
    "cspell": "^8.8.1",
    "daisyui": "^4.12.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.4",
    "lefthook": "^1.6.16",
    "next-superjson-plugin": "^0.6.3",
    "postcss": "^8.4.38",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "prisma": "^5.15.0",
    "react-email": "^2.1.2",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "wireit": "^0.14.4",
    "zenstack": "^2.2.3"
  },
  "shipzen": {
    "version": "0.1.4"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "app:build"
      ]
    },
    "dev": {
      "dependencies": [
        "app:dev"
      ]
    },
    "lint": {
      "command": "eslint . --fix",
      "dependencies": [
        "zenstack:generate"
      ]
    },
    "pretty": {
      "command": "prettier --write ."
    },
    "start": {
      "dependencies": [
        "app:start"
      ]
    },
    "test": {
      "dependencies": [
        "lint",
        "pretty",
        "typecheck"
      ]
    },
    "typecheck": {
      "command": "tsc --noEmit",
      "dependencies": [
        "zenstack:generate"
      ]
    },
    "app:dev": {
      "command": "next dev -p 3000",
      "service": true,
      "dependencies": [
        "zenstack:generate"
      ]
    },
    "app:build": {
      "command": "next build",
      "dependencies": [
        "zenstack:generate"
      ],
      "output": [
        ".next/**/*"
      ],
      "clean": false
    },
    "app:start": {
      "command": "next start -p 3000",
      "service": true
    },
    "ci:test": {
      "dependencies": [
        "ci:lint",
        "ci:pretty",
        "ci:typecheck"
      ]
    },
    "ci:lint": {
      "command": "eslint .",
      "dependencies": [
        "zenstack:generate"
      ]
    },
    "ci:pretty": {
      "command": "prettier --check ."
    },
    "ci:typecheck": {
      "command": "tsc --noEmit",
      "dependencies": [
        "zenstack:generate"
      ]
    },
    "mailer:dev": {
      "command": "email dev --dir=./src/mailer/emails",
      "service": true
    },
    "zenstack:generate": {
      "command": "zenstack generate",
      "files": [
        "prisma/**/*",
        "schema.zmodel"
      ],
      "output": [
        "node_modules/.prisma/**/*",
        "node_modules/.zenstack/**/*"
      ]
    }
  }
}
