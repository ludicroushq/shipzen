    "cpx2": "^7.0.1",
    "cspell": "^8.8.0",
    "lefthook": "^1.6.10",
    "next-superjson-plugin": "^0.6.3",
    "postcss": "^8.4.38",
    "prisma": "^5.13.0",
    "superjson": "2.2.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "wireit": "^0.14.4",
    "zenstack": "^2.0.3"
  },
  "shipzen": {
    "version": "0.0.9"
  },
  "wireit": {
    "build": {
      "dependencies": ["app:build"]
    },
    "dev": {
      "dependencies": ["app:dev"]
    },
    "lint": {
      "command": "biome check . --apply-unsafe",
      "dependencies": ["zenstack:generate"]
    },
    "spellcheck": {
      "command": "cspell lint . --quiet"
    },
    "start": {
      "dependencies": ["app:start"]
    },
    "test": {
      "dependencies": ["lint", "spellcheck", "typecheck"]
    },
    "typecheck": {
      "command": "tsc --noEmit",
      "dependencies": ["zenstack:generate"]
    },
    "app:dev": {
      "command": "next dev -p 3000",
      "service": true,
      "dependencies": ["zenstack:generate"]
    },
    "app:build": {
      "command": "next build",
      "dependencies": ["zenstack:generate"],
      "output": [".next/**/*"]
    },
    "app:start": {
      "command": "next start -p 3000",
      "service": true
    },
    "ci:test": {
      "dependencies": ["ci:lint", "ci:spellcheck", "ci:typecheck"]
    },
    "ci:lint": {
      "command": "biome check .",
      "dependencies": ["zenstack:generate"]
    },
    "ci:spellcheck": {
      "command": "cspell lint . --quiet"
    },
    "ci:typecheck": {
      "command": "tsc --noEmit",
      "dependencies": ["zenstack:generate"]
    },
    "zenstack:generate": {
      "command": "zenstack generate",
      "files": ["prisma/**/*", "schema.zmodel"],
      "output": ["node_modules/.prisma/**/*", "node_modules/.zenstack/**/*"]
    }
  }
}
