    "nodemailer": "^6.9.13",
    "oslo": "^1.2.0",
    "prisma-json-schema-generator": "^5.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.4",
    "react-icons": "^5.2.0",
    "sharp": "^0.33.3",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.3.0",
    "tslog": "^4.9.2",
