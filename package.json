{
	"name": "TODO",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"app:build": "wireit",
		"app:dev": "wireit",
		"app:start": "wireit",
		"build": "wireit",
		"ci:test": "wireit",
		"dev": "wireit",
		"lint": "wireit",
		"mailer:dev": "wireit",
		"spellcheck": "wireit",
		"start": "wireit",
		"test": "wireit",
		"typecheck": "wireit"
	},
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^4.0.1",
		"@nextui-org/react": "^2.3.6",
		"@premieroctet/next-admin": "^4.2.0",
		"@prisma/client": "^5.13.0",
		"@react-email/components": "^0.0.17",
		"@sentry/nextjs": "^8.0.0",
		"@t3-oss/env-nextjs": "^0.9.2",
		"@zenstackhq/runtime": "^2.1.0",
		"framer-motion": "^11.2.0",
		"lucia": "^3.2.0",
		"ms": "^2.1.3",
		"next": "^14.2.3",
		"next-safe-action": "^6.2.0",
		"nodemailer": "^6.9.13",
		"oslo": "^1.2.0",
		"prisma-json-schema-generator": "^5.1.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-icons": "^5.2.1",
		"sharp": "^0.33.3",
		"sonner": "^1.4.41",
		"tailwind-merge": "^2.3.0",
		"tslog": "^4.9.2",
		"uuid": "^9.0.1",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.7.3",
		"@tailwindcss/typography": "^0.5.13",
		"@types/ms": "^0.7.34",
		"@types/node": "^20.12.12",
		"@types/nodemailer": "^6.4.15",
		"@types/react": "^18.3.2",
		"@types/react-dom": "^18.3.0",
		"@types/uuid": "^9.0.8",
		"autoprefixer": "^10.4.19",
		"cpx2": "^7.0.1",
		"cspell": "^8.8.1",
		"lefthook": "^1.6.11",
		"next-superjson-plugin": "^0.6.3",
		"postcss": "^8.4.38",
		"prisma": "^5.13.0",
		"react-email": "^2.1.2",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.5",
		"wireit": "^0.14.4",
		"zenstack": "^2.1.0"
	},
	"shipzen": {
		"version": "0.0.12"
	},
	"wireit": {
		"build": {
			"dependencies": ["app:build"]
		},
		"dev": {
			"dependencies": ["app:dev"]
		},
		"lint": {
			"command": "biome check . --apply-unsafe",
			"dependencies": ["zenstack:generate"]
		},
		"spellcheck": {
			"command": "cspell lint . --quiet"
		},
		"start": {
			"dependencies": ["app:start"]
		},
		"test": {
			"dependencies": ["lint", "spellcheck", "typecheck"]
		},
		"typecheck": {
			"command": "tsc --noEmit",
			"dependencies": ["zenstack:generate"]
		},
		"app:dev": {
			"command": "next dev -p 3000",
			"service": true,
			"dependencies": ["zenstack:generate"]
		},
		"app:build": {
			"command": "next build",
			"dependencies": ["zenstack:generate"],
			"output": [".next/**/*"]
		},
		"app:start": {
			"command": "next start -p 3000",
			"service": true
		},
		"ci:test": {
			"dependencies": ["ci:lint", "ci:spellcheck", "ci:typecheck"]
		},
		"ci:lint": {
			"command": "biome check .",
			"dependencies": ["zenstack:generate"]
		},
		"ci:spellcheck": {
			"command": "cspell lint . --quiet"
		},
		"ci:typecheck": {
			"command": "tsc --noEmit",
			"dependencies": ["zenstack:generate"]
		},
		"mailer:dev": {
			"command": "email dev --dir=./src/mailer/emails",
			"service": true
		},
		"zenstack:generate": {
			"command": "zenstack generate",
			"files": ["prisma/**/*", "schema.zmodel"],
			"output": ["node_modules/.prisma/**/*", "node_modules/.zenstack/**/*"]
		}
	}
}
