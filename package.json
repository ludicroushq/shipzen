{
  "name": "SHIPZEN",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "app:build": "wireit",
    "app:dev": "wireit",
    "app:start": "wireit",
    "ci:test": "wireit",
    "dev": "wireit",
    "lint": "wireit",
    "pretty": "wireit",
    "start": "wireit",
    "test": "wireit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@nextui-org/react": "^2.2.10",
    "@premieroctet/next-admin": "^3.6.0",
    "@prisma/client": "^5.12.1",
    "@react-email/components": "^0.0.16",
    "@sentry/nextjs": "^7.109.0",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tanstack/react-query": "^5.29.0",
    "@tanstack/react-query-devtools": "^5.29.0",
    "@trpc/client": "^11.0.0-next-beta.316",
    "@trpc/react-query": "^11.0.0-next-beta.316",
    "@trpc/server": "^11.0.0-next-beta.316",
    "@zenstackhq/runtime": "^2.0.0-beta.8",
    "cookies-next": "^4.1.1",
    "framer-motion": "^11.0.25",
    "lucia": "^3.1.1",
    "ms": "^2.1.3",
    "next": "^14.1.4",
    "next-superjson-plugin": "0.6.1",
    "nodemailer": "^6.9.13",
    "oslo": "^1.2.0",
    "prisma-json-schema-generator": "3.1.5",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.2",
    "react-icons": "^5.0.1",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.2.2",
    "tslog": "^4.9.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zod-validation-error": "^3.1.0"
  },
  "devDependencies": {
    "@cspell/dict-typescript": "^3.1.2",
    "@cspell/eslint-plugin": "^8.6.1",
    "@next/eslint-plugin-next": "^14.1.4",
    "@tailwindcss/typography": "^0.5.12",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.12.5",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/parser": "^7.5.0",
    "@vercel/style-guide": "^6.0.0",
    "autoprefixer": "^10.4.19",
    "cspell": "^8.6.1",
    "eslint": "^8",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "prisma": "^5.12.1",
    "superjson": "^2.2.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.4",
    "wireit": "^0.14.4",
    "zenstack": "^2.0.0-beta.8"
  },
  "wireit": {
    "dev": {
      "dependencies": [
        "app:dev"
      ]
    },
    "lint": {
      "command": "eslint . --fix --max-warnings 0",
      "dependencies": [
        "zenstack:generate"
      ]
    },
    "pretty": {
      "command": "prettier --write ."
    },
    "start": {
      "dependencies": [
        "app:start"
      ]
    },
    "test": {
      "dependencies": [
        "lint",
        "pretty"
      ]
    },
    "app:dev": {
      "command": "next dev -p 3000",
      "service": true,
      "dependencies": [
        "zenstack:generate"
      ]
    },
    "app:build": {
      "command": "next build"
    },
    "app:start": {
      "command": "next start -p 3000",
      "service": true
    },
    "ci:test": {
      "dependencies": [
        "ci:lint",
        "ci:pretty"
      ]
    },
    "ci:lint": {
      "command": "eslint . --max-warnings 0",
      "dependencies": [
        "zenstack:generate"
      ]
    },
    "ci:pretty": {
      "command": "prettier --check ."
    },
    "zenstack:generate": {
      "command": "zenstack generate"
    }
  }
}
