    "zod": "^3.23.6",
    "zod-validation-error": "^3.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.2",
    "@tailwindcss/typography": "^0.5.13",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.12.8",
    "@types/nodemailer": "^6.4.15",
