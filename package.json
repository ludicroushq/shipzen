    "@tanstack/react-query-devtools": "^5.35.1",
    "@trpc/client": "^11.0.0-rc.354",
    "@trpc/react-query": "^11.0.0-rc.354",
    "@trpc/server": "^11.0.0-rc.354",
    "@zenstackhq/runtime": "^2.0.3",
    "cookies-next": "^4.1.1",
    "framer-motion": "^11.1.7",
    "lucia": "^3.2.0",
    "ms": "^2.1.3",
